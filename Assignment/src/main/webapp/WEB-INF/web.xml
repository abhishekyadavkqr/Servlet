<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
 id="WebApp_ID" 
 version="4.0">

	<welcome-file-list>
	<welcome-file>HomePage.asp</welcome-file>
	</welcome-file-list>



 <!-- configuration of Servlet Name -->
 <servlet>
 <servlet-name>Meal_message</servlet-name>
 <servlet-class>com.assignment.servlet.Assignment1</servlet-class>
 </servlet>
 
 <!-- Mapping Servlet -->
 
 <servlet-mapping>
 <servlet-name>Meal_message</servlet-name>
 <url-pattern>/assignment1</url-pattern>
 </servlet-mapping>
 
 
 
 
 <!-- configuration of Servlet Name -->
 <servlet>
 <servlet-name>Marriage</servlet-name>
 <servlet-class>com.assignment2.servlet.Assignment2</servlet-class>
 </servlet>
 
 <!-- Mapping Servlet -->
 
 <servlet-mapping>
 <servlet-name>Marriage</servlet-name>
 <url-pattern>/assignment2</url-pattern>
 </servlet-mapping>
 
 <!-- Assignment2 html page configuration -->
 <servlet>
 <servlet-name>home</servlet-name>
  <jsp-file>/WEB-INF/AssignPages/Assign2/HomePage.html</jsp-file>
 </servlet>
 
 <!--Assignment2 html page Mapping-->
 
 <servlet-mapping>
 <servlet-name>home</servlet-name>
 <url-pattern>/HomePage.asp</url-pattern>
 
 </servlet-mapping>
 
 
 
</web-app>